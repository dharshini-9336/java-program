import java.util.Scanner;
public class main{
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        if(n>0){         
             String numStr = String.valueOf(n);
            int count = numStr.length();
            System.out.printf("The Count the digits: "+count);

        }
    }
}
