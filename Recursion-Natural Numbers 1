import java.util.Scanner;
public class main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();  
        if(num >= 1 && num <= 50) {
            System.out.println("The first " + num + " Natural Numbers are:");
            for(int i = 1; i <= num; i++) {
                System.out.print(i + " ");
            }
        } else {
            System.out.println("Enter a Valid Input!!!!!!!!!!!!!!!!!!!!!!!!");
        }
    }
}
