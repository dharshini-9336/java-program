import java.util.Scanner;
public class main{
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       String n=sc.nextLine();
        String reversed="";
        for(int i=n.length()-1;i>=0;i--){
            reversed+=n.charAt(i);
        }
        System.out.println(reversed);
        }
        
}
