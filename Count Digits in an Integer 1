import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long n = sc.nextLong();
        int count=0;
        long temp=n;
        while(temp>0){
            count++;
            temp /=10;
        } if (n>=1 && n<=10000000){
                System.out.println("The count of the given integer is: " +count);
            } 
         else {
            System.out.println("Enter a Valid Input");
         }
    }   
}
